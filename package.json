{
  "name": "my-codemod",
  "type": "module",
  "bin": {
    "my-codemod": "dist/cli.js"
  },
  "scripts": {
    "generate": "tsdown --no-config src/generator.ts && node dist/generator",
    "build": "node --run generate && tsdown && chmod +x dist/cli.js",
    "lint": "eslint . --ext .ts",
    "test": "vitest",
    "postinstall": "node --run generate"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "eslint": "^9.26.0",
    "eslint-config-vuetify": "^4.0.0",
    "esprima": "^4.0.1",
    "tsdown": "^0.11.9",
    "typescript": "^5.4.3",
    "vitest": "^2.1.5",
    "vue-metamorph": "^3.3.3"
  }
}
